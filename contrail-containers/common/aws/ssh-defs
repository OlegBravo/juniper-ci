ENV_FILE="$WORKSPACE/cloudrc"
SSH_OPTS="-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -o ServerAliveInterval=30"
SCP="scp -i $WORKSPACE/kp $SSH_OPTS"

source $ENV_FILE
SSH_DEST="$SSH_USER@$public_ip_cloud"
SSH="ssh -i $WORKSPACE/kp $SSH_OPTS $SSH_DEST"

SSH_DEST_BUILD="$SSH_USER@$public_ip_build"
SSH_BUILD="ssh -i $WORKSPACE/kp $SSH_OPTS $SSH_DEST_BUILD"
