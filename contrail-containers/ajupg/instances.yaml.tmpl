provider_config:
 bms:
   domainsuffix: local
   ssh_user: ${SSH_USER}
   ssh_pwd:

instances:
 server1:
  ip: ${IP_CONT_01}
  provider: bms
  roles:
   analytics: null
   analytics_database: null
   config: null
   config_database: null
   control: null
   webui: null

global_configuration:
 REGISTRY_PRIVATE_INSECURE: True
 CONTAINER_REGISTRY: ${CONTAINER_REGISTRY}

contrail_configuration:
 CONTRAIL_VERSION: ${CONTRAIL_VERSION}
 CONTROLLER_NODES: ${IP_CONT_01}
 CONTROL_NODES: ${IP2_CONT_01}
 AUTH_MODE: keystone
 KEYSTONE_AUTH_ADMIN_PASSWORD: password
 KEYSTONE_AUTH_HOST: ${AUTH_IP}
 KEYSTONE_AUTH_URL_VERSION: "/v3"
 KEYSTONE_AUTH_USER_DOMAIN_NAME: "admin_domain"
 KEYSTONE_AUTH_PROJECT_DOMAIN_NAME: "admin_domain"
 RABBITMQ_NODE_PORT: 5673
 IPFABRIC_SERVICE_IP: ${METADATA_IP}
 AAA_MODE: cloud-admin
 CONFIG_NODEMGR__DEFAULTS__minimum_diskGB: "2"
 DATABASE_NODEMGR__DEFAULTS__minimum_diskGB: "2"
 JVM_EXTRA_OPTS: "-Xms1g -Xmx2g"
 LOG_LEVEL: SYS_DEBUG
