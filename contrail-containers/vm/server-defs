export NET_COUNT=1
export NET_BASE_PREFIX=50

# overrides
export CONT_NODE_MEM=4096
export CONT_NODE_MEM=4096

if [[ "$HA" == 'ha' ]]; then
  export CONT_NODES=3
  export COMP_NODES=2
elif [[ "$HA" == 'nonha' ]]; then
  export CONT_NODES=1
  export COMP_NODES=2
else
  echo "ERROR: \"$HA\" deployment is not supported"
  exit 1
fi
